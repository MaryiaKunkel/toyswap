{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/listingNew.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "description": {
      "type": "string",
      "minLength": 1
    },
    "price": {
      "type": "integer",
      "minimum": 0
    },
    "image_url": {
      "type": "string",
      "format": "uri"
    }
  },
  "additionalProperties": false,
  "required": ["title", "description", "price", "image_url"]
}
